[program:websrd]
command=gunicorn websrd.wsgi --bind 0.0.0.0 --certfile=/etc/letsencrypt/live/<%=@site_name%>/fullchain.pem --keyfile=/etc/letsencrypt/live/<%=@site_name%>/privkey.pem
directory=/var/www/opensrd-api
autostart=true
autorestart=true
stderr_logfile=/var/log/websrd_django.err.log
stdout_logfile=/var/log/websrd_django.out.log